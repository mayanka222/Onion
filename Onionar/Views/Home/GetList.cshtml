@model IEnumerable<ViewModel.VmStudent>
@{
    ViewData["Title"] = "GetList";
    int i = 0;
}

@if(TempData["msg"]!=null)
            {
                <h1>@TempData["msg"]</h1>
            }

<h1>GetList</h1>
@if (Model != null && Model.Count() > 1)
{
    <table id="MyTable" class="table">
        <thead>
            <tr>Name</tr>
            <tr>Coures</tr>
            <tr>RollNo</tr>
            <tr>Batch</tr>
            <tr>Action</tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                i++;
                <tr>
                    <td>@i</td>
                    <td>@item.Name</td>
                    <td>@item.Coures</td>
                    <td>@item.RollNo</td>
                    <td>@item.Batch</td>
                    <td><a class="btn btn-danger" href="/home/deleteStudentByid?id=@item.ID">Delete</a></td>
                </tr>
            }

        </tbody>
    </table>
}
else
{
    <h1>No Data Found </h1>
}

